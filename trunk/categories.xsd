	<xsd:annotation>
		<xsd:documentation xml:lang="en">
			IMPORT-EXPORT XMLSchema-categories
		</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="categories">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="category" type="categoryType" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="categoryType">
		<xsd:all>
			<xsd:element name="id" type="xsd:unsignedLong" minOccurs="0" />
			<xsd:element name="name" type="char255Type" />
			<xsd:element name="description" type="xsd:string" />
			<xsd:element name="parent" type="xsd:unsignedLong" />
			<xsd:element name="sortorder" type="xsd:unsignedLong" />
			<xsd:element name="coursecount" type="xsd:unsignedLong" minOccurs="0" />
			<xsd:element name="visible" type="xsd:boolean" minOccurs="0" />
			<xsd:element name="timemodified" type="xsd:unsignedLong" minOccurs="0" />
		</xsd:all>
	</xsd:complexType>
	<xsd:simpleType name="char255Type">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255" />
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
